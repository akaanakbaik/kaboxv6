generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Media {
  id        String   @id @default(uuid())
  name      String
  url       String
  size      Int
  mimeType  String
  provider  String
  createdAt DateTime @default(now())
}
